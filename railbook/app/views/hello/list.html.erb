<table border="1">
  <tr>
    <th>ISBNコード</th><th>署名</th><th>価格</th>
    <th>出版社</th><th>刊行日</th><th>CD-ROM</th>
  </tr>

<% @books.each do |book| %>
  <tr>
    <td style="background-color:<%= cycle('#FFfFF', '#FFff99) %>">
      <%= book.isbn %>
    </td>
    <td style="background-color:<%= current-cycle %>">
      <%= book.title %>
    </td>
    <td><%= book.price %></td>
    <td><%= book.publish %></td>
    <td><%= book.published %></td>
    <td><%= book.cd %></td>
  </tr>
<% end %>
</table>
<%= debug(@books) %>
